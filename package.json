{
  "name": "prettier-plugin-ink",
  "version": "1.1.1",
  "description": "prettier plugin for inkle's ink",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run test",
    "test": "tap --no-coverage",
    "test-watch": "TAP_SNAPSHOT=1 tap -w",
    "test-init": "curl https://raw.githubusercontent.com/inkle/the-intercept/master/Assets/Ink/TheIntercept.ink > intercept.ink"
  },
  "dependencies": {
    "inkjs": "^2.2.1",
    "line-column": "^1.0.2"
  },
  "files": [
    "index.js",
    "lib/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/little-bonsai/prettier-plugin-ink.git"
  },
  "keywords": [
    "prettier",
    "ink"
  ],
  "author": {
    "name": "Freddie Gilbraith",
    "email": "freddie.gilbraith@littlebonsai.co.uk",
    "url": "https://littlebonsai.co.uk/"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "tap": "^16.3.6"
  },
  "tap": {
    "check-coverage": false
  }
}
